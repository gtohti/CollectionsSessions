package practiceByDay48;

public class StudentTester {
public static void main(String[] args) {
	
	Person p1 = new Person();
	p1.name = "Gheyret Tohti";
	p1.age = 52;
	p1.height = 5.5f;
	p1.gender = "Male";
	
	System.out.println("The student is: "  + p1.gender + ", name is:" + p1.name + ", age is :" + p1.age);
	p1.eat();
	Student s1 = new Student();
	
	s1.name = "Awat Tohti";
	s1.age = 25;
	s1.height = 5.6f;
	s1.gender = "Female";
	s1.studentId = "AT34234Cybertek";
	s1.studentId = "GT234542Cybertek";
	s1.studenType = "Under Gradute";
	
	System.out.println("The student is: "  + s1.gender + ", name is:" 
	+ s1.name + ", and student ID is: " + s1.studentId + ", age is :" + s1.age);
	
	p1.eat();
   }
}
