package MyPracticJava;

import java.util.Arrays;

public class MyPracticJavaMethos {
public static void main(String[] args) {
	


	double[] nums1 = {1, 3, 34, 5, 86} ; 
    double[] nums2 = {2, 4, 2, 88};
    double[] combine = combine(nums1,nums2);
    //System.out.println(Arrays.toString(nums1) + Arrays.toString(nums2));
  
  }
  
  public static double[] combine(double[] d1, double[] d2) {
    int size = d1.length+d2.length;
    double[] combined = new double[size];
    
    for (int i = 0; i < d1.length; i++) {
		combined[i] = d1[i];
	}
    for (int j = 0; j < d2.length; j++) {
    	combined[d1.length + j] = d2[j];
	}
    return combined;
    
  }
  
 
    }


