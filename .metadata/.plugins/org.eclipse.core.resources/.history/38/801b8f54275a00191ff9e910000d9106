package replPractice;

import java.util.Scanner;

public class replPractice84 {
	
	public static void main(String[] args) {
		Scanner scan = new Scanner (System.in);
		int n = scan.nextInt();
		int result = 0;
		
		for (int i = 1; i <= n; i++) {
		result = result *i;
	
				System.out.print(result);
			}
			
		}
		
	}
	
	


