package replPractice;

import java.util.Scanner;

public class replPractice84 {
	
	public static void main(String[] args) {
		Scanner scan = new Scanner (System.in);
		int n = scan.nextInt();
		int fact = n*(n+1);
		
		for (int i = 1; i <= n; i++) {
			for (int j = 1; j <= i; j++) {
				System.out.print(fact);
			}
			System.out.println();
		}
		
	}
	
	

}
